{
    "[typescript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "eslint.format.enable": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact",
      ],
    "eslint.run": "onType",
    "eslint.runtime": "node",
    "eslint.codeActionsOnSave.mode": "all"

}